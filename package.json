    "commander": "^2.7.1",
    "file": "^0.2.2",
    "inquirer": "^0.11.0",
    "jquery": "^2.1.1",
    "lodash": "^3.6.0",
    "node-uuid": "^1.4.3",
    "q": "^1.2.0",
    "selenium-webdriver": "^2.48.2"
  },
  "devDependencies": {
    "chai-as-promised": "^5.1.0",
    "cucumber": "^0.9.0",
    "istanbul": "^0.4.1",
    "jscs": "^2.1.1",

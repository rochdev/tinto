    "test": "./node_modules/.bin/jshint lib test --reporter=node_modules/jshint-stylish && ./node_modules/.bin/jscs lib test && node --harmony ./node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --recursive lib test"
